(defun precondition (but br)
  (remove nil (mapcar (lambda(x)(if(find but (cadr x))
                                    (car x)
                                  nil)
                        )
                br)
          )
  )

(defun chainageArriere (bf br but)
  (let(trouve
       (pre_list (precondition but br))
       )
    (if (null pre_list)
        (if (find but bf) (setq trouve T) 
                          (setq trouve nil))
        (dolist (x pre_list nil)
          (if 
            (dolist (a x T)
              (if (not (chainageArriere (bf br a))
                       (setq trouve nil)
                       ))
              (setq trouve T)
              )
            trouve))))
  )

